{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": ["error", {
      "ignore": [
        "history",
        "match"
      ]
    }],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
